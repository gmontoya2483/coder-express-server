{
  "type": "module",
  "name": "servidor-express",
  "version": "1.0.0",
  "description": "Pug",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:fork": "nodemon server.js",
    "dev:fork:8081": "nodemon server.js -p 8081 -m FORK",
    "dev:cluster":"nodemon server.js -m CLUSTER",
    "dev:cluster:8081": "nodemon server.js -p 8081 -mode CLUSTER",
    "forever:8080": "forever --watch start server.js -p 8080 -mode FORK",
    "forever:8081": "forever --watch start server.js -p 8081 -mode FORK",
    "pm2:fork:8080": "pm2 start server.js --watch --name='ecommerce-8080' -- -p 8080 -m FORK",
    "pm2:fork:8081": "pm2 start server.js --watch --name='ecommerce-8081' -- -p 8081 -m FORK",
    "pm2:cluster:8080": "pm2 start server.js --watch -i max --name='ecommerce-cluster-8080' -- -p 8080",
    "pm2:cluster:8081": "pm2 start server.js --watch -i max --name='ecommerce-cluster-8081' -- -p 8081",
    "start": "node server.js",
    "build:db": "node src/batch/createDbTables.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^7.6.2",
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "knex": "^2.3.0",
    "minimist": "^1.2.7",
    "mongoose": "^6.6.7",
    "mysql": "^2.18.1",
    "normalizr": "^3.6.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.5.1",
    "uuid": "^9.0.0"
  }
}
